<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.librarybookmanage">
<uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@drawable/books"
        android:label="@string/app_name"
        android:roundIcon="@drawable/books"
        android:supportsRtl="true"
        android:theme="@style/Theme.LibraryBookManage">
        <activity android:name=".MainActivity">
        </activity>
        <activity android:name=".AddBookDetails" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".UpdateBookDetails" />
        <activity android:name=".ViewDetails" />
    </application>

</manifest>